{
  "publisher": "christianbrassat",
  "name": "linq",
  "displayName": "linQ",
  "description": "",
  "version": "0.1.4",
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.registerSitemap",
    "onCommand:extension.insertLink"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.registerSitemap",
        "title": "linQ: Register Sitemap"
      },
      {
        "command": "extension.insertLink",
        "title": "linQ: Insert Link"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "@types/request-promise": "^4.1.42",
    "@types/request-promise-native": "^1.0.15",
    "node-html-parser": "^1.1.11"
  }
}
